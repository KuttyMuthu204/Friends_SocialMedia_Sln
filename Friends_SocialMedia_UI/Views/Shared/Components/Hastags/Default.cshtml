@model List<Hastag>

<div class="flex justify-between text-black">
	<div class="font-bold text-base">Trends for you</div>
</div>

<div class="space-y-3.5 capitalize text-xs font-normal mt-5 mb-2 text-gray-600">
	@foreach (var hashtag in Model)
	{
		<a>
			<div class="flex items-center gap-3 p">
				<div class="flex-1">
					<div class="font-semibold text-black">@hashtag.Name</div>
					<div class="mt-0.5 mb-2">@hashtag.Count</div>
				</div>
			</div> 
		</a>
	}
</div>